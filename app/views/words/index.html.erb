<div class="word-container">
  <% @words.each do |word|  %>
    <div class="card" style="width: 18rem;">
    <img src="..." class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title"><%= word.english %></h5>
        <p class="card-text"><%= word.japanese %></p>
      </div>
    </div>
  <% end %>
</div>


